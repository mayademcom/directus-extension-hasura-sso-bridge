{
	"name": "@mayademcom/directus-extension-hasura-sso-bridge",
	"description": "Seamless SSO bridge between Hasura and Directus admin panel.",
	"icon": "extension",
	"version": "1.0.0",
	"author": "Mayadem",
	"license": "MIT",
	"readmeFilename": "README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/mayademcom/directus-extension-hasura-sso-bridge"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-endpoint"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"release": "release-it"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.1.1",
		"@release-it/conventional-changelog": "^10.0.1",
		"@types/express": "^5.0.3",
		"@types/jsonwebtoken": "^9.0.0",
		"@types/node": "^24.0.0",
		"conventional-changelog-cli": "^5.0.0",
		"release-it": "^19.0.3",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2"
	}
}
